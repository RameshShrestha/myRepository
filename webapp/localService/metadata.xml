<edmx:Edmx
    xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
    xmlns:sap="http://www.sap.com/Protocols/SAPData" Version="1.0">
    <edmx:DataServices
        xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" m:DataServiceVersion="2.0">
        <Schema
            xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
            xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
            xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="zrIBM.oDataService.calcService">
            <EntityType Name="calcViewType">
                <Key>
                    <PropertyRef Name="EMPID"/>
                </Key>
                <Property Name="EMPID" Type="Edm.String" Nullable="false" MaxLength="3" sap:label="ID"/>
                <Property Name="EMPNAME" Type="Edm.String" MaxLength="100" sap:label="NAME"/>
                <Property Name="EMPDATE" Type="Edm.DateTime" sap:display-format="Date" sap:label="DATE"/>
                <Property Name="SAL" Type="Edm.Int32" sap:label="SALARY"/>
                <Property Name="DEPTNAME" Type="Edm.String" MaxLength="20" sap:label="DEPARTMENT"/>
                <Property Name="BONUS" Type="Edm.Int32" sap:label="BONUS"/>
            </EntityType>
            <EntityContainer Name="calcService" m:IsDefaultEntityContainer="true">
                <EntitySet Name="calcView" EntityType="zrIBM.oDataService.calcService.calcViewType"/>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>